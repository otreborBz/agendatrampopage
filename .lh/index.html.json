onial h4 { font-weight: bold; }\r\n\r\n    /* CTA */\r\n    .cta { padding: 80px 0; text-align: center; background: #7ed957; color: #fff; }\r\n    .cta h2 { margin-bottom: 30px; }\r\n    \r\n    /* Footer */\r\n    footer { padding: 40px 0; text-align: center; background: #333; color: #fff; }\r\n    footer a { color: #7ed957; }\r\n  </style>\r\n</head>\r\n<body>\r\n\r\n  <!-- Hero -->\r\n  <section class=\"hero container\">\r\n    <div class=\"hero-text\">\r\n      <h1>Agenda Trampo</h1>\r\n      <p>Organize seus serviços de forma rápida e prática: nomes, endereços, valores, datas, horários e contatos.</p>\r\n      <a href=\"#\" class=\"btn\">Testar grátis no Android</a>\r\n    </div>\r\n    <div class=\"hero-image\">\r\n      <img src=\"sua-screenshot.png\" alt=\"Screenshot do app\">\r\n    </div>\r\n  </section>\r\n\r\n  <!-- Funcionalidades -->\r\n  <section class=\"features\">\r\n    <div class=\"container\">\r\n      <h2>Funcionalidades</h2>\r\n      <div class=\"feature-grid\">\r\n        <div class=\"feature\">\r\n          <h3>Organização completa</h3>\r\n          <p>Gerencie nome, endereço, valor e tipo de serviço facilmente.</p>\r\n        </div>\r\n        <div class=\"feature\">\r\n          <h3>Controle de datas e horários</h3>\r\n          <p>Agende seus serviços com precisão e receba lembretes importantes.</p>\r\n        </div>\r\n        <div class=\"feature\">\r\n          <h3>Contatos centralizados</h3>\r\n          <p>Tenha todos os telefones e informações de clientes organizadas.</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n\r\n  <!-- Depoimentos -->\r\n  <section class=\"testimonials\">\r\n    <div class=\"container\">\r\n      <h2>O que dizem nossos usuários</h2>\r\n      <div class=\"testimonial-grid\">\r\n        <div class=\"testimonial\">\r\n          <p>\"O Agenda Trampo facilitou muito minha rotina de serviços!\"</p>\r\n          <h4>Lucas M.</h4>\r\n        </div>\r\n        <div class=\"testimonial\">\r\n     